{
	"name": "codebuddha.io",
	"version": "0.0.8",
	"description": "An open repository of basic applications for developers.",
	"main": "index.js",
	"scripts": {
		"preversion": "echo \"{\\\"node_version\\\":\\\"$(node -v)\\\",\\\"npm_version\\\":\\\"v$(npm -v)\\\"}\" | tee status.json",
		"prod": "webpack --env=prod --mode production",
		"dev": "webpack-dev-server --env=dev --mode development",
		"uglify": "uglifyjs -c -m -o dist/js/bundle.js -- dist/js/bundle.js",
		"clean": "npm outdated && npm prune",
		"server": "cd dist/ && http-server -a 0.0.0.0 -p 8080 -c-1",
		"serve": "npm run server",
		"start": "npm run dev",
		"test": "mocha --trace-warnings"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/iSkore/codebuddha.io.git"
	},
	"keywords": [
		"basic",
		"developer",
		"apps"
	],
	"author": "Nick Soggin",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/iSkore/codebuddha.io/issues"
	},
	"homepage": "https://github.com/iSkore/codebuddha.io#readme",
	"dependencies": {
		"axios": "^0.18.0",
		"bootstrap": "^4.0.0",
		"faster-crc32": "0.0.5",
		"font-awesome": "^4.7.0",
		"jquery": "^3.3.1",
		"popper.js": "^1.12.9"
	},
	"devDependencies": {
		"autoprefixer": "^8.0.0",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.3",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-stage-2": "^6.24.1",
		"babelify": "^8.0.0",
		"chai": "^4.1.2",
		"chai-as-promised": "^7.1.1",
		"copy-webpack-plugin": "^4.4.2",
		"css-loader": "^0.28.10",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"file-loader": "^1.1.10",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.0.0",
		"json-loader": "^0.5.7",
		"mocha": "^5.0.1",
		"node-sass": "^4.7.2",
		"nodemon": "^1.17.1",
		"optimize-css-assets-webpack-plugin": "^3.2.0",
		"postcss-loader": "^2.1.1",
		"precss": "^3.1.2",
		"raw-loader": "^0.5.1",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.20.2",
		"uglifyjs-webpack-plugin": "^1.2.2",
		"url-loader": "^0.6.2",
		"webpack": "^4.0.1",
		"webpack-cli": "^2.0.9",
		"webpack-dev-server": "^3.1.0",
		"webpack-merge": "^4.1.2"
	}
}
